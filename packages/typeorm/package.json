{
  "name": "@fsmoothy/typeorm",
  "version": "2.2.0",
  "private": false,
  "author": "Vassiliy Kuzenkov (bondiano)",
  "license": "MIT",
  "description": "Strong typed state machine for your TypeORM Entities",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fsmoothy/fsmoothy.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.mjs",
  "types": "dist/index.d.ts",
  "source": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "typeorm",
    "fsm",
    "state-machine"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "npm run build -- --watch",
    "prebuild": "npm run clean",
    "build": "tsc --project tsconfig.build.json",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "coverage": "vitest --coverage",
    "docs:build": "typedoc src/index.ts --tsconfig tsconfig.build.json",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "typeorm": "^0.3.20"
  },
  "commit": "cz",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@fsmoothy/core": "1.4.0"
  },
  "files": [
    "/package.json",
    "/dist"
  ],
  "devDependencies": {
    "@electric-sql/pglite": "^0.2.15",
    "@types/node": "^22.10.5",
    "typeorm-pglite": "^0.3.2"
  }
}
